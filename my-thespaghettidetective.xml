<?xml version="1.0" encoding="utf-8"?>
<Container>
    <Name>TheSpaghettiDetective</Name>
    <Description>
    [b][u]This Container needs a Redis and a database![/u][/b][br]
    Whether you're worried about print failures, fire hazards, or network security, The Spaghetti Detective has got your back.
    </Description>
    <GitHub>https://github.com/thespaghettidetective/thespaghettidetective</GitHub>
    <Repository>thespaghettidetective/thespaghettidetective</Repository>
    <Project>https://www.thespaghettidetective.com/</Project>
    <BindTime>true</BindTime>
    <Privileged>false</Privileged>
    <Environment>
        <Variable>
            <Name>UMASK</Name>
            <Value>000</Value>
        </Variable>
        <Variable>
            <Name>PUID</Name>
            <Value>99</Value>
        </Variable>
        <Variable>
            <Name>PGID</Name>
            <Value>100</Value>
        </Variable>
    </Environment>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>3334</HostPort>
                <ContainerPort>3334</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/thespaghettidetective</HostDir>
            <ContainerDir>/app</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Version>05616fd2</Version>
    <WebUI>http://[iP]:[PORT:3334]/</WebUI>
    <Banner>http://whatsmybeeragain.com/wp-content/uploads/2013/12/Whats-My-Beer-Logo-Banner-2-01-e1318366883801.png</Banner>
    <Icon>https://avatars.githubusercontent.com/u/46323662?s=400&amp;v=4</Icon>
    <ExtraParams></ExtraParams>
    <Config Name="Email Host" Target="EMAIL_HOST" Default="" Mode="" Description="SMTP Host" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Email User" Target="EMAIL_HOST_USER" Default="" Mode="" Description="SMTP User" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Email Password" Target="EMAIL_HOST_PASSWORD" Default="" Mode="" Description="SMTP Password" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Email From" Target="DEFAULT_FROM_EMAIL" Default="" Mode="" Description="Sender adress" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="EMAIL Use TLS" Target="EMAIL_USE_TLS" Default="True" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Use Https" Target="SITE_USES_HTTPS" Default="False" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Allow Social login" Target="SOCIAL_LOGIN" Default="False" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Redis URL" Target="REDIS_URL" Default="redis://" Mode="" Description="IP and port of your Redis instance" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Database URL" Target="DATABASE_URL" Default="sqlite://" Mode="" Description="IP and port of your database" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Machine Learning API" Target="ML_API_HOST" Default="http://" Mode="" Description="IP and port of your machine learning api" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Allow sign up" Target="ACCOUNT_ALLOW_SIGN_UP" Default="False" Mode="" Description="Set to 'True' if you want to open sign up form" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="Internal media host" Target="INTERNAL_MEDIA_HOST" Default="http://[iP]:[PORT:3334]" Mode="" Description="" Type="Variable" Display="advanced-hide" Required="true" Mask="false"></Config>
    <Config Name="Enable Debug" Target="DEBUG" Default="False" Mode="" Description="Enable Debug mode" Type="Variable" Display="advanced-hide" Required="true" Mask="false"></Config>
</Container>